public String whichTwo(Integer a, Integer b, Integer c) {
    //code here
    String result = '';
    Integer d = a+b;
    Integer e = b+c;
    Integer f = a+c;
    if(d==c){
        result = 'c';
    }else if(e==a){
        result = 'a';
    }else if(f==b){
        result = 'b';
    }
    return result;
}