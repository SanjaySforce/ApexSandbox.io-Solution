public String convert15to18DigitId(String fifteenDigit)
{
    //code here
    if(fifteenDigit.length() != 15 || fifteenDigit == ''){
        return '-1';
    }else if(fifteenDigit == null){
        return null;
    }else{
        Id eighteenDigit = fifteenDigit;
        return eighteenDigit;
    }
}