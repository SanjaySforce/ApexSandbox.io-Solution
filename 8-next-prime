public Integer nextPrime(Integer num) {
    //code here
   Integer next = num + 1;
   Boolean isPrime = true;
   if(num<=1){
       next = 2;
   }else{
        while (true) {
                for (Integer i = 2; i <= next; i++) {
                    if (Math.mod(next, i) == 0) {
                        isPrime = false;
                        break;
                    }
                }
            next++;
        }
   }
  return next;
}