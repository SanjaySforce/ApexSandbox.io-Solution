public Boolean isKeyAccount(Account a) {
    //code here
    Boolean isKeyAccount = false;
    
    if(a != null && a.Industry != null && a.AnnualRevenue != null){
        if(acc.Industry == 'Banking') {
            isKeyAccount = acc.AnnualRevenue >= 600000;
        } else if(acc.Industry == 'Technology') {
            isKeyAccount = acc.AnnualRevenue >= 800000;
        } else if(acc.Industry == 'Retail') {
            isKeyAccount = acc.AnnualRevenue >= 2000000;
        } else {
            isKeyAccount = acc.AnnualRevenue >= 500000;
        }
    }
    return isKeyAccount;
}