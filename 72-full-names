public List<String> fullNames(List<String> firstNames, List<String> lastNames) {
    //code here
    List<String> fullNames = new List<String>();
    if(firstNames.size() > 0 && lastNames.size() > 0){
        for(String fName: firstNames){
            for(String lName: lastNames){
                fullNames.add(fName+' '+lName);
            }
        }
    }
}