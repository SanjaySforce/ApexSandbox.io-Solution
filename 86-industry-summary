public Map<String, Decimal> industrySummary(List<Account> accounts) {
    //code here
    Map<String, Decimal> industrySummaryMap = new Map<String, Decimal>();
    if(accounts.size() > 0){
        for(account a: accounts){
            if(a.Industry == 'Banking'){
                Decimal industrySummary =+ a.AnnualRevenue;
                industrySummaryMap.put(a.Industry,industrySummary);
            }else if(a.Industry == 'Retail'){
                Decimal retailSummary =+ a.AnnualRevenue;
                industrySummaryMap.put(a.Industry,retailSummary);
            }else{
                Decimal technologySummary =+ a.AnnualRevenue;
                industrySummaryMap.put(a.Industry,technologySummary);
            }
        }
    }
   return  industrySummaryMap;
}
