public Integer nextPrime(Integer num) {
   //code here
   Integer nxtPrime = 0;
   Integer next = num+1;
   if(num <= 1){
       nxtPrime = 2;
   }
   Boolean isPrime = true;
   while(isPrime){
        for(Integer i=2; i<next; i++){
            if(Math.mod(num,i) == 0){
                isPrime = false;
                break;
            }
        }
        next++;
   }
   nxtPrime = next;
   return nxtPrime;
}