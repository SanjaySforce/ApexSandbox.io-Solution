public List<sObject> getListOfsObject(List<Account> accounts, List<Contact> contacts)
{
    // code here
    List<sObject> wrapObj = new List<sObject>();
    if(accounts == null){
        if(contacts == null){
            return wrapObj;
        }else{
            wrapObj.addAll(contacts);
            return wrapObj;
        }
    }else{
        wrapObj.addAll(accounts);
        wrapObj.addAll(contacts);
        return wrapObj;
    }
}