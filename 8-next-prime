public Integer nextPrime(Integer num) {
    //code here
    boolean isPrime = false;
    Integer next = num + 1;
    while (true) {
        if (primeOrNot(next)) {
            return next;
        }
        next++;
    }
public boolean primeOrNot(Integer n){
    boolean isPrime = false;
     for(Integer i=2; i<n; i++){
        if(Math.mod(n,i) != 0){
            isPrime = true;
        }
}