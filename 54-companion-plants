public Boolean companionPlants(String plant1, String plant2) {
    //code here
    Boolean isCompanion = false;
    if(player1 == 'lettuce' && player2 == 'cucumbers'){
        isCompanion = true
    }else if(player1 == 'lettuce' && player2 == 'onions'){
        isCompanion = true;
    }else if(player1 == 'onions' && player2 == 'carrots'){
        isCompanion = true;
    }else if(player1 == 'onions' && player2 == 'tomatoes'){
        isCompanion = true;
    }
    return isCompanion;
}