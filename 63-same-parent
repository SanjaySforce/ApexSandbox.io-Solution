public Boolean sameParent(Contact c, Opportunity opp) {
    //code here
    Boolean isSameParent = false;
    if(c != null){
        if(opp != null){
            if(c.AccountId != null && opp.AcountId != null 
                && c.AccountId == opp.AccountId){
                isSameParent = true;
            }
        }
    }
    return isSameParent;
}