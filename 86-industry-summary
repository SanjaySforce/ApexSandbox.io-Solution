public Map<String, Decimal> industrySummary(List<Account> accounts) {
    //code here
    Map<String, Decimal> industrySummaryMap = new Map<String, Decimal>();
    Decimal totalSummary = 0;
    List<String> industryList = new list<String>();
    if(accounts.size() > 0){
        for(account a: accounts){
            totalSummary =+ a.AnnualRevenue;
            industryList.add(a.Industry);
        }
        if(industryList.size() > 0 ){
            for(String ind: industryList){
                industrySummaryMap.put(ind,totalSummary);
            }
        }
    }
   return  industrySummaryMap;
}
