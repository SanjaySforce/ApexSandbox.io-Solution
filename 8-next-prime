public Integer nextPrime(Integer num) {
    //code here
         Integer next = num + 1;
        while (true) {
            if (isPrime(next)) {
                return next;
            }
            next++;
        }
}
 public static Boolean isPrime(Integer n) {
        if (n <= 1) {
            return false;
        }
        for (Integer i = 2; i <= Math.sqrt(n); i++) {
            if (Math.mod(n, i) == 0) {
                return false;
            }
        }
        return true;
    }